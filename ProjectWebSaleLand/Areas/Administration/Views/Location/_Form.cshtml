@model ProjectWebSaleLand.Shared.Model.Location.LocationModels
@{
    Html.EnableUnobtrusiveJavaScript(true);
    var _ID = Model.ID;
}
@Html.HiddenFor(model => model.ID)
<div class="row form-horizontal form-label-left" style="margin-bottom:15px;">   
    @*Name*@
    <div class="form-group">
        <label class="control-label col-md-3 col-sm-3 col-xs-12 lineHeight">Tên khu vực: </label>
        <div class="col-md-6 col-sm-6 col-xs-12">
            @Html.TextBoxFor(model => model.Name, new { @class = "form-control has-feedback-left", placeholder = "", autocomplete = "off" })
            <span class="fa fa-user form-control-feedback left" aria-hidden="true"></span>
            @Html.ValidationMessageFor(model => model.Name, null, new { @class = "label label-danger" })
        </div>
    </div>
</div>

<script type="text/javascript">
    //=========================================
    $(document).ready(function () {
        
    });
</script>